{
    "expo": {
        "name": "Loucas & Lisas",
        "slug": "loucaselisasnative",
        "owner": "agenciaboz-dev",
        "scheme": "loucaseco",
        "version": "1.0.153",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#000"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "userInterfaceStyle": "light",
            "supportsTablet": false,
            "bundleIdentifier": "com.agenciabozdev.loucaseco",
            "associatedDomains": ["applinks:app.loucaseco.com.br"]
        },
        "android": {
            "userInterfaceStyle": "light",
            "adaptiveIcon": {
                "foregroundImage": "./assets/foreground-icon.png",
                "monochromeImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#000"
            },
            "softwareKeyboardLayoutMode": "resize",
            "package": "com.agenciabozdev.loucaseco",
            "googleServicesFile": "./google-services.json"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            ["./plugins/withDisableForcedDarkModeAndroid.js", {}],
            ["./plugins/withAndroidLargeHeap.js", {}],
            [
                "expo-image-picker",
                {
                    "photosPermission": "The app accesses your photos to let you upload it in your profile."
                }
            ],
            [
                "expo-font",
                {
                    "fonts": [
                        "assets/fonts/custom/FoundersGroteskBOZ-Regular.otf",
                        "assets/fonts/custom/FoundersGroteskBOZ-Bold.otf",
                        "assets/fonts/custom/FoundersGroteskXCondBOZ-Bold.otf",
                        "assets/fonts/FoundersGrotesk-Regular.otf",
                        "assets/fonts/FoundersGrotesk-Bold.otf",
                        "assets/fonts/FoundersGroteskXCond-Bold.otf"
                    ]
                }
            ],
            [
                "expo-notifications",
                {
                    "icon": "./assets/notification-icon.png",
                    "color": "#000000"
                }
            ]
        ],
        "extra": {
            "eas": {
                "projectId": "4dd09afc-0e4d-4b39-bfd3-8b67e07148b3"
            }
        }
    }
}
  